<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.password">Ketonal</property>
        <property name="hibernate.connection.url">jdbc:mysql://127.0.0.1:3388/mobiway_db</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="show_sql">false</property>
        <mapping class="com.company.model.User"></mapping>
        <mapping class="com.company.model.Journey"></mapping>
        <mapping class="com.company.model.JourneyData"></mapping>
        <mapping class="com.company.model.UserContact"></mapping>
        <mapping class="com.company.model.Location"></mapping>
        <mapping class="com.company.model.Favorite"></mapping>
    </session-factory>
</hibernate-configuration>
